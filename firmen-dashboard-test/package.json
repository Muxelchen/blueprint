{
  "name": "firmen-dashboard-test",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "blueprint": "./cli/blueprint-cli.ts"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cli": "tsx cli/blueprint-cli.ts",
    "blueprint": "tsx cli/blueprint-cli.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dev-check": "tsx cli/dev-check.ts check",
    "dev-fix": "tsx cli/dev-check.ts fix",
    "dev-setup": "npm run dev-fix && npm run dev-check",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "deps-update": "npm update && npm audit fix",
    "analyze": "ANALYZE=true npm run build",
    "analyze:serve": "npm run analyze && npx serve dist",
    "perf": "npm run build && npm run analyze",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "precommit": "npm run format && npm run lint && npm run type-check",
    "build:production": "NODE_ENV=production npm run build",
    "deploy:preview": "npm run build && npx vercel --prod",
    "docker:build": "docker build -t blueprint-ui .",
    "docker:run": "docker run -p 3000:3000 blueprint-ui",
    "components:check": "tsx cli/dev-check.ts check-components",
    "components:create": "tsx cli/blueprint-cli.ts generate component",
    "templates:list": "tsx cli/blueprint-cli.ts list",
    "templates:create": "tsx cli/blueprint-cli.ts create",
    "optimize": "npm run clean && npm ci && npm run build:production",
    "debug": "DEBUG=true npm run dev",
    "benchmark": "tsx scripts/benchmark.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/leaflet": "^1.9.18",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "commander": "^11.0.0",
    "date-fns": "^3.0.6",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.4",
    "html2canvas": "^1.4.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.279.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.5.1",
    "react-hot-toast": "^2.5.2",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.6.2",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "recharts": "^2.8.0",
    "tsx": "^4.0.0",
    "uuid": "^11.1.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/html2canvas": "^0.5.35",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/ui": "^3.2.2",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.29",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "serve": "^14.2.4",
    "tailwindcss": "^3.3.3",
    "terser": "^5.41.0",
    "typescript": "^5.0.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.2"
  }
}