{
  "name": "blueprint-ui-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "blueprint": "./cli/blueprint-cli.ts"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cli": "tsx cli/blueprint-cli.ts",
    "blueprint": "tsx cli/blueprint-cli.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dev-check": "tsx cli/dev-check.ts check",
    "dev-fix": "tsx cli/dev-check.ts fix",
    "dev-setup": "npm run dev-fix && npm run dev-check",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "deps-update": "npm update && npm audit fix"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.18",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "commander": "^11.0.0",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.4",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.279.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.8.0",
    "tsx": "^4.0.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
